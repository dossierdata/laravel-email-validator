{
  "name": "dossierdata/laravel-email-validator",
  "description": "A more extensive email validator rule than the default provided by Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Lucas van der Have",
      "email": "lucas@dossierdata.nl"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "egulias/email-validator": "^2.1 || ^3.0",
    "mika56/spfcheck": "^1.1",
    "laravel/framework": "^6.0 || ^7.0 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phpspec/phpspec": "~4.0",
    "friendsofphp/php-cs-fixer": "^2.8",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Dossierdata\\LaravelEmailValidator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dossierdata\\LaravelEmailValidator\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dossierdata\\LaravelEmailValidator\\ServiceProvider",
        "Dossierdata\\LaravelEmailValidator\\BootServiceProvider"
      ]
    }
  }
}
