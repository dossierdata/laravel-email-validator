{
  "name": "dossierdata/laravel-email-validator",
  "description": "A more extensive email validator rule than the default provided by Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Lucas van der Have",
      "email": "lucas@dossierdata.nl"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "egulias/email-validator": "~2.1",
    "mika56/spfcheck": "^1.0",
    "laravel/laravel": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "phpspec/phpspec": "~2.1",
    "friendsofphp/php-cs-fixer": "^2.8",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Dossierdata\\LaravelEmailValidator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dossierdata\\LaravelEmailValidator\\Tests\\": "tests"
    }
  }
}
